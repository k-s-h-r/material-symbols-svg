{
  "name": "material-symbols-react-monorepo",
  "private": true,
  "scripts": {
    "build:dev": "pnpm --filter=\"*-core\" build && pnpm -r --filter=\"./packages/*\" --filter=\"!*-core\" build:dev",
    "build": "pnpm --filter=\"*-core\" build && pnpm -r --filter=\"./packages/*\" --filter=\"!*-core\" build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "publish-packages": "pnpm -r publish --access public"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
