{
  "name": "@material-symbols-svg/vue-sharp",
  "version": "0.1.11",
  "description": "Material Symbols (Sharp) as Vue components with TypeScript support",
  "type": "module",
  "main": "./dist/w400.js",
  "types": "./dist/w400.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/w400.d.ts",
      "import": "./dist/w400.js"
    },
    "./w100": {
      "types": "./dist/w100.d.ts",
      "import": "./dist/w100.js"
    },
    "./w200": {
      "types": "./dist/w200.d.ts",
      "import": "./dist/w200.js"
    },
    "./w300": {
      "types": "./dist/w300.d.ts",
      "import": "./dist/w300.js"
    },
    "./w400": {
      "types": "./dist/w400.d.ts",
      "import": "./dist/w400.js"
    },
    "./w500": {
      "types": "./dist/w500.d.ts",
      "import": "./dist/w500.js"
    },
    "./w600": {
      "types": "./dist/w600.d.ts",
      "import": "./dist/w600.js"
    },
    "./w700": {
      "types": "./dist/w700.d.ts",
      "import": "./dist/w700.js"
    },
    "./icons/*": {
      "types": "./dist/icons/*.d.ts",
      "import": "./dist/icons/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm run clean && node ../../scripts/sync-dependencies.cjs && node ../../scripts/generate-icons.cjs sharp vue && node ../../scripts/generate-exports.cjs sharp vue && pnpm run build:lib",
    "build:dev": "pnpm run clean && node ../../scripts/sync-dependencies.cjs && NODE_ENV=development node ../../scripts/generate-icons.cjs sharp vue && node ../../scripts/generate-exports.cjs sharp vue && pnpm run build:lib",
    "build:lib": "rollup -c",
    "clean": "rimraf dist",
    "test": "echo \"No tests yet\"",
    "lint": "cd ../.. && eslint packages/vue-sharp/src --ext .ts"
  },
  "keywords": [
    "vue",
    "vue3",
    "icons",
    "material-symbols",
    "sharp",
    "typescript",
    "components",
    "svg"
  ],
  "author": "h-s-h-r",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/k-s-h-r/material-symbols-svg.git",
    "directory": "packages/vue-sharp"
  },
  "bugs": {
    "url": "https://github.com/k-s-h-r/material-symbols-svg/issues"
  },
  "homepage": "https://material-symbols-svg.com",
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.3.3",
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@material-symbols/svg-100": "^0.33.0",
    "@material-symbols/svg-200": "^0.33.0",
    "@material-symbols/svg-300": "^0.33.0",
    "@material-symbols/svg-400": "^0.33.0",
    "@material-symbols/svg-500": "^0.33.0",
    "@material-symbols/svg-600": "^0.33.0",
    "@material-symbols/svg-700": "^0.33.0"
  }
}
